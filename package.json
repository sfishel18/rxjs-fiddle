    "lint": "tslint -c tslint.json \"./**/*.{ts,tsx}\" -e \"./{build,node_modules}/**\"",
    "storybook": "start-storybook -p 6006",
    "test:visual": "build-storybook && loki --requireReference --reactUri file:./storybook-static",
